{
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "name": "Tag-a-Doc",
  "short_name": "TagaDoc",
  "description": "This extension displays document tags / properties for G-Suite / Google Apps",
  "version": "1.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "oauth2": {
    "client_id": "840861035525-2i3gkqc7ridmotb39vtba1mib3ja7cgo.apps.googleusercontent.com",
    "scopes": [
      "email", "profile", "https://www.googleapis.com/auth/drive.readonly"
    ]
  },
  "permissions": ["identity", "tabs", "https://docs.google.com/*", "https://www.googleapis.com/*"],
  "content_scripts": [
      {
    "matches": [
      "*://docs.google.com/*"
    ],
    "js": [
      "script.js"
    ]
  }],
  "background": {
    "scripts": ["network.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "TagaDoc",
      "default_icon": "icon16.png",
      "default_popup": "settings.html"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "key": ""
}